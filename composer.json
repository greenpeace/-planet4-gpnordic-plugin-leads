{
    "name": "greenpeace/planet4-gpnordic-plugin-gutenberg-blocks",
    "description": "Greenpeace Nordic specific Gutenberg customisations and additions to the core stack.",
    "authors": [
        {
            "name": "Greenpeace Nordic",
            "email": "skoleva@greenpeace.org"
        }
    ],
    "type": "wordpress-plugin",
    "minimum-stability" : "stable",
    "require": {},
    "require-dev": {
      "squizlabs/php_codesniffer": "^3.5.3",
      "wp-coding-standards/wpcs": "^2.2.0",
      "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
      "stevegrunwell/phpunit-markup-assertions": "^1.2",
      "phpunit/phpunit": "^6.2"
    },
    "config": {
      "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "P4GPNGBKS\\": [
                "classes"
            ],
            "P4GPNGBKS\\Blocks\\": [
                "classes/blocks"
            ]
        }
    },
    "scripts": {
      "fixes": "vendor/squizlabs/php_codesniffer/bin/phpcbf",
      "sniffs": "vendor/squizlabs/php_codesniffer/bin/phpcs"
    }
}
